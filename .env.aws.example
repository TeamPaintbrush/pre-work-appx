# AWS Configuration for Template Storage
# Add these variables to your .env.local file

# AWS Region
AWS_REGION=us-east-1

# AWS Credentials (Use IAM user with appropriate permissions)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# DynamoDB Table for Template Storage
AWS_DYNAMODB_TEMPLATES_TABLE=prework-templates

# S3 Bucket for Template File Storage
AWS_S3_TEMPLATES_BUCKET=prework-templates-storage

# Optional: Custom endpoint for local development (localstack)
# AWS_ENDPOINT_URL=http://localhost:4566

# Optional: Enable AWS SDK debug mode
# AWS_SDK_LOAD_CONFIG=1
# AWS_SDK_LOG_LEVEL=debug
