{
  "name": "the-pre-work-app",
  "version": "1.0.0",
  "description": "The Pre-Work App - Checklist for project management with React Server components.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build && echo 'âœ… Build complete! Ready for AWS Amplify deployment.'",
    "deploy:amplify": "npm run build && amplify publish",
    "deploy:setup": "amplify init --yes",
    "amplify:init": "amplify init --yes",
    "amplify:push": "amplify push --yes",
    "type-check": "tsc --noEmit",
    "test:aws": "cd . && tsx scripts/tests/test-aws-integration.ts",
    "setup:aws": "node scripts/setup-aws-template-storage.js",
    "setup:aws-help": "echo 'Please follow AWS_TEMPLATE_STORAGE_IMPLEMENTATION.md for setup instructions'",
    "migrate:templates": "echo 'Use the AWS Template Dashboard to migrate templates'",
    "setup:enterprise": "node scripts/setup-enterprise-tables.js",
    "enterprise:enable": "cp .env.local.enterprise .env.local && echo 'Enterprise features enabled! Restart your dev server.'",
    "enterprise:disable": "echo '# Enterprise features disabled' > .env.local"
  },
  "keywords": [
    "react",
    "nextjs",
    "checklist",
    "project management",
    "prework"
  ],
  "author": "Your Name",
  "license": "MIT",
