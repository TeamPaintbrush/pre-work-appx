# Staging Environment Variables
NODE_ENV=development
NEXT_PUBLIC_APP_URL=https://staging-prework-app.vercel.app
NEXT_PUBLIC_API_URL=https://staging-api.prework-app.com

# Database Configuration
DATABASE_URL=your_staging_database_url
REDIS_URL=your_staging_redis_url

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_staging_aws_access_key
AWS_SECRET_ACCESS_KEY=your_staging_aws_secret_key
AWS_S3_BUCKET=prework-app-assets-staging
AWS_DYNAMODB_TABLE=prework-checklists-staging

# Authentication
NEXTAUTH_URL=https://staging-prework-app.vercel.app
NEXTAUTH_SECRET=your_staging_nextauth_secret
JWT_SECRET=your_staging_jwt_secret

# External Services
SENDGRID_API_KEY=your_staging_sendgrid_api_key
SENTRY_DSN=your_staging_sentry_dsn
GOOGLE_ANALYTICS_ID=your_staging_google_analytics_id

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_MONITORING=true
ENABLE_ERROR_REPORTING=true
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_DEBUG_MODE=true

# Performance
MAX_UPLOAD_SIZE=10485760
API_RATE_LIMIT=200
CACHE_TTL=1800

# Security
CORS_ORIGIN=https://staging-prework-app.vercel.app
ALLOWED_ORIGINS=https://staging-prework-app.vercel.app,http://localhost:3000
CSP_REPORT_URI=https://staging-monitoring.prework-app.com/csp-report

# Monitoring
MONITORING_WEBHOOK_URL=your_staging_monitoring_webhook_url
HEALTH_CHECK_INTERVAL=15000
LOG_LEVEL=debug
